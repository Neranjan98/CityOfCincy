@page
@model CityOfCincy.Pages.ChicagoDataModel
@{
    ViewData["Title"] = "Chicago Housing Information";
    var OptionSet = (List<String>)ViewData["ChicagoDataOptions"];
}

<div class="left">
    @foreach (var item in OptionSet)
    {
        <a href="/ChicagoDataDetailed?status=@item">
            <div class="card" style="margin-top:6px">
                <div class="card-body">
                    @item
                </div>
            </div>
        </a>
    }
</div>
