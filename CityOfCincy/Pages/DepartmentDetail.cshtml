@using CityOfCincy.Models;
@model CityOfCincy.Pages.DepartmentDetailModel
@{
    ViewData["Title"] = "Department Information";
    var DepartmentData = (DepartmentInfoResponse)ViewData["departmentInfo"];
    var SalaryData = (List<EmployeeSalaryResponse>)ViewData["salaryInfo"];
}

Department - @DepartmentData.department <br />
Average Salary - @SalaryData.FirstOrDefault().annual_rt<br />
Positions - @SalaryData.Count()<br />
Role - @SalaryData.FirstOrDefault().business_title <br />

<iframe width="600"
     height="450"
     style="border:0"
     loading="lazy"
     allowfullscreen
     referrerpolicy="no-referrer-when-downgrade"
