@page
@model CityOfCincy.Pages.DepartmentsModel
@using CityOfCincy.Models;
@{
    ViewData["Title"] = "Departments";
    var deptList = (List<DepartmentInfoResponse>)ViewData["departmentData"];
}

<div style="overflow-y:hidden;">
    <div class="left">

        </tr>
        @{
            foreach (var item in deptList)
            {
                <a href="/DepartmentDetail?inputDept=@item.department_codes&stringName=@item.department">
                    <div class="card" style="margin-top:6px">
                        <div class="card-body">

                            @item.department

                        </div>

                    </div>
                </a>
            }

        }

    </div>
