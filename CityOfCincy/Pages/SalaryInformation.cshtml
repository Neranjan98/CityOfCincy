@page
@model CityOfCincy.Pages.SalaryInformationModel
@using CityOfCincy.Utilities;
@using CityOfCincy.Models;
@{
    ViewData["Title"] = "Salary Information";
    List<EmployeeSalaryResponse> employeeSalaryResponse = (List<EmployeeSalaryResponse>)ViewData["salaryResult"];
    var groupedResult = employeeSalaryResponse.GroupBy(x => x.deptname);
}

<div style="overflow-y:hidden;">
    <div class="left">

        </tr>
        @{
            foreach (var item in groupedResult)
            {
                <a href="/SalaryInformationDetail?inputDept=@item.FirstOrDefault().deptid">
                        <div class="card" style="margin-top:6px">
                            <div class="card-body">

                                @item.Key

                            </div>

                        </div>
                    </a>
            }



        }

    </div>
